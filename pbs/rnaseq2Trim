#PBS -q small

PPN=8
PID=${PID:-acc4}
JOB=${JOB:-rnaseq2TrimJob}

module load fastqc/0.11.2

dirw=$data/misc2/rnaseq/${PID}
cd $dirw

cat 12.fastqc.sh 14.trim.sh 16.fastqc.sh > 18.sh
parallel -j $PPN < 18.sh

touch $dirw/cps/${JOB}
